{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article",
        "description": ""
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "slug": {
            "type": "string",
            "required": true,
            "unique": true
        },
        "title": {
            "type": "string",
            "required": true
        },
        "content": {
            "type": "richtext"
        },
        "tags": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::tag.tag"
        },
        "thumbnail": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::article-thumbnail.article-thumbnail"
        }
    }
}

{
	"parser": "@babel/eslint-parser",
	"extends": "eslint:recommended",
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true,
		"browser": false
	},
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"jsx": false
		},
		"sourceType": "module"
	},
	"globals": {
		"strapi": true
	}
}

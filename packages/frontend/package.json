		"@types/react-dom": "^17.0.11",
		"@types/styled-components": "^5.1.22",
		"babel-jest": "^28.0.3",
		"babel-loader": "^8.2.5",
		"babel-plugin-styled-components": "^2.0.7",
		"babel-plugin-twin": "^1.0.2",
		"babel-preset-react-app": "^10.0.1",
		"concurrently": "^7.1.0",
		"cross-env": "^7.0.3",
		"postcss": "^8.4.13",
		"typescript": "4.6.4"

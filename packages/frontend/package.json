{
	"name": "@andrevital.com/frontend",
	"private": true,
	"version": "0.1.0",
	"scripts": {
		"dev": "yarn start:dev",
		"build": "next build",
		"start": "next start",
		"start:dev": "concurrently -k \"next dev -p $(grep '^PORT=' <.env.development | cut -d= -f2)\"",
		"typecheck": "echo frontend && tsc --pretty",
		"typecheck:watch": "yarn typecheck --watch",
		"lint": "eslint --cache --ext .ts,.tsx .",
		"prepare": "cross-env-shell ../../scripts/yarn-prepare.sh"
	},
	"dependencies": {
		"@react-hook/window-size": "^3.0.7",
		"@tailwindcss/typography": "^0.5.2",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^14.1.1",
		"autoprefixer": "^10.4.6",
		"classnames": "^2.3.1",
		"imagesloaded": "^5.0.0",
		"lightgallery": "^2.4.0",
		"framer-motion": "^6.3.3",
		"lodash": "^4.17.21",
		"next": "12",
		"react": "^17.0.2",
		"react-cool-onclickoutside": "^1.7.0",
		"react-dom": "^17.0.2",
		"react-scripts": "5.0.1",
		"styled-components": "^5.3.5",
		"tailwindcss": "^3.0.24",

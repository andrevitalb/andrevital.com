{
	"name": "@andrevital.com/frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"codegen": "ts-graphql-plugin typegen",
		"codegen:watch": "nodemon --ext ts,tsx --exec yarn codegen --ignore '/__generated__/'",
		"dev": "yarn start:dev",
		"lint": "eslint --cache --ext .ts,.tsx .",
		"prepare": "cross-env-shell ../../scripts/yarn-prepare.sh",
		"start": "next start",
		"start:dev": "concurrently -k \"next dev -p $(grep '^PORT=' <.env.development | cut -d= -f2)\"",
		"typecheck": "echo frontend && tsc --pretty",
		"typecheck:watch": "yarn typecheck --watch"
	},
	"dependencies": {
		"@apollo/client": "^3.7.17",
		"@react-hook/window-size": "^3.1.1",
		"@tailwindcss/typography": "^0.5.9",
		"autoprefixer": "^10.4.14",
		"classnames": "^2.3.2",
		"framer-motion": "^10.13.0",
		"graphql": "^16.7.1",
		"imagesloaded": "^5.0.0",
		"lightgallery": "^2.7.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"next": "13.4.10",
		"react": "^18.2.0",
		"react-cool-onclickoutside": "^1.7.0",
		"react-dom": "^18.2.0",
		"react-markdown": "^8.0.7",
		"react-moment": "^1.1.3",
		"react-scripts": "5.0.1",
		"react-syntax-highlighter": "^15.5.0",
		"remark-gfm": "^3.0.1",
		"styled-components": "^6.0.4",
		"tailwindcss": "^3.3.3",
		"twin.macro": "^3.3.1",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@next/eslint-plugin-next": "^13.4.10",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@types/imagesloaded": "^4.1.2",
		"@types/jest": "^29.5.3",
		"@types/node": "^20.4.2",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-syntax-highlighter": "^15.5.7",
		"@types/styled-components": "^5.1.26",
		"babel-jest": "^29.6.1",
		"babel-loader": "^9.1.3",
		"babel-plugin-styled-components": "^2.1.4",
		"babel-plugin-twin": "^1.1.0",
		"babel-preset-react-app": "^10.0.1",
		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"eslint-plugin-graphql": "^4.0.0",
		"nodemon": "^3.0.1",
		"postcss": "^8.4.26",
		"ts-graphql-plugin": "^3.0.1",
		"typescript": "5.1.6"
	}
}
